{
    "name": "umairhanif/laravel-table-logger",
    "description": "Automatically log Laravel model changes with dynamic table creation",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "laravel/framework": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "UmairHanif\\LaravelTableLogger\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "UmairHanif\\LaravelTableLogger\\LaravelTableLoggerServiceProvider"
            ],
            "aliases": {
                "TableLogger": "UmairHanif\\LaravelTableLogger\\Facades\\TableLogger"
            }
        }
    },
    "authors": [
        {
            "name": "Umair Hanif",
            "email": "umair2054@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": false
}